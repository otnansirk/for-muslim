{
    "name": "For Muslim",
    "description": "Muslim app to make it more obedient and better than yesterday.",
    "version": "1.0.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html",
        "default_title": "For Muslim"
    },
    "icons": {
        "192": "/assets/favicon/web-app-manifest-192x192.png",
        "512": "/assets/favicon/web-app-manifest-512x512.png"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; font-src 'self'; style-src 'self' 'unsafe-inline';"
    },
    "permissions": [
        "storage"
    ]
}